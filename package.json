    "http-server": "^0.12.1",
    "jest": "^25.1.0",
    "jest-preset-angular": "^8.0.0",
    "prettier": "^1.19.1",
    "ts-node": "~8.6.2",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "~4.0.7"
  }
