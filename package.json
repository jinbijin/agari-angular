{
  "name": "agari-angular",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4242",
    "start:ci": "ng serve --port=4242",
    "start:serve": "http-server -p 8080 -c-1 dist/agari-angular",
    "build": "ng build",
    "cy": "cypress open",
    "cy:verify": "cypress verify",
    "cy:ci": "cypress run --record",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "ng lint",
    "graphql-generate": "graphql-codegen --config graphql-codegen.yml"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.0",
    "@angular/cdk": "^11.2.7",
    "@angular/common": "^12.1.0",
    "@angular/compiler": "^12.1.0",
    "@angular/core": "^12.1.0",
    "@angular/forms": "^12.1.0",
    "@angular/material": "^11.2.7",
    "@angular/platform-browser": "^12.1.0",
    "@angular/platform-browser-dynamic": "^12.1.0",
    "@angular/router": "^12.1.0",
    "@angular/service-worker": "^12.1.0",
    "@apollo/client": "^3.0.0",
    "@ngxs/store": "^3.7.1",
    "apollo-angular": "^2.4.0",
    "file-saver": "^2.0.5",
    "graphql": "^15.0.0",
    "idb": "^6.0.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "xlsx": "^0.16.9",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.0",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/cli": "^12.1.0",
    "@angular/compiler-cli": "^12.1.0",
    "@angular/language-service": "^12.1.0",
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-apollo-angular": "^2.3.2",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@types/jest": "^26.0.22",
