    "graphql-tag": "^2.10.3",
    "idb": "^5.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "xlsx": "^0.15.5",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.7",
