{
  "name": "agari-angular",
  "version": "2.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4242",
    "start:ci": "ng serve --port=4242",
    "start:serve": "http-server -p 8080 -c-1 dist/agari-angular",
    "build": "ng build",
    "cy": "cypress open",
    "cy:verify": "cypress verify",
    "cy:ci": "cypress run --record",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "ng lint",
    "graphql-generate": "graphql-codegen --config graphql-codegen.yml"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "moduleNameMapper": {
      "src(.*)": "<rootDir>/src/$1"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.1",
    "@angular/cdk": "^12.2.1",
    "@angular/common": "^12.2.1",
    "@angular/compiler": "^12.2.1",
    "@angular/core": "^12.2.1",
    "@angular/forms": "^12.2.1",
    "@angular/material": "^12.2.1",
    "@angular/platform-browser": "^12.2.1",
    "@angular/platform-browser-dynamic": "^12.2.1",
    "@angular/router": "^12.2.1",
    "@angular/service-worker": "^12.2.1",
    "@ngxs/store": "^3.7.2",
    "file-saver": "^2.0.5",
    "idb": "^6.1.2",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "xlsx": "^0.17.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.1",
    "@angular-eslint/builder": "^12.3.1",
    "@angular-eslint/eslint-plugin": "^12.3.1",
    "@angular-eslint/eslint-plugin-template": "^12.3.1",
    "@angular-eslint/schematics": "^12.3.1",
    "@angular-eslint/template-parser": "^12.3.1",
    "@angular/cli": "^12.2.1",
    "@angular/compiler-cli": "^12.2.1",
    "@angular/language-service": "^12.2.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.5",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "cypress": "^7.7.0",
    "eslint": "^7.30.0",
    "eslint-plugin-import": "2.23",
    "eslint-plugin-jsdoc": "^35.4.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "7.24",
    "http-server": "^0.12.3",
    "jest": "^27.0.6",
    "jest-preset-angular": "^9.0.4",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "typescript": "~4.3.4"
  }
}
